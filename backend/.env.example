# Server Configuration
NODE_ENV=development
PORT=5000
LOG_LEVEL=info

# Canvas API Configuration
CANVAS_API_BASE_URL=https://dlsl.instructure.com/api/v1
CANVAS_API_TOKEN=your_canvas_token_here
CANVAS_API_TIMEOUT=30000
CANVAS_MAX_RETRIES=3
CANVAS_RETRY_DELAY=1000
CANVAS_PER_PAGE=100

# Cache Configuration
CACHE_ENABLED=true
CACHE_DEFAULT_TTL=300
CACHE_CHECK_PERIOD=600
CACHE_TTL_USER=600
CACHE_TTL_COURSES=300
CACHE_TTL_ASSIGNMENTS=180
CACHE_TTL_SUBMISSIONS=120
CACHE_TTL_ANNOUNCEMENTS=120
CACHE_TTL_ACTIVITY=120

# Security Configuration
READ_ONLY_MODE=true
FRONTEND_URL=http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX=100
